loom = require("./loom")
say = loom.say
act = loom.act

loom.start(function()
    say("A")
    say("B")
    say("C")
    act(function()
        say("  D")
        say("  E")
        say("  F")
        act(function()
            say("    G")
            say("    H")
            say("    I")
        end)
    end)
    say("K")
    say("L")
    say("M")
    act(function()
        say("  N")
        say("  O")
        say("  P")
    end)
end)